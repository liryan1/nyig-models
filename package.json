{
  "name": "@nyig/models",
  "version": "0.5.4",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --write src",
    "lint": "pnpm run format && tsc",
    "prepare": "husky",
    "prepublishOnly": "cp package.json README.md dist",
    "release": "pnpm run build && changeset publish"
  },
  "peerDependencies": {
    "zod": ">=3.25.46"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts,md}": "prettier --write"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
