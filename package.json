{
  "name": "@nyig/models",
  "version": "0.6.14",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/liryan1/nyig-models.git"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "provenance": true
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --write src",
    "lint": "pnpm run format && tsc",
    "prepare": "husky",
    "prepublishOnly": "cp package.json README.md dist",
    "release": "pnpm run build && changeset publish"
  },
  "peerDependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts,md}": "prettier --write"
  }
}